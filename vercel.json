{
  "version": 2,
  "framework": null,
  "routes": [
    { "src": "/api/auth/signup", "dest": "/api/auth/signup.js" },
    { "src": "/api/auth/signin", "dest": "/api/auth/signin.js" },
    { "src": "/api/memories(.*)", "dest": "/api/memories/index.js" },
    { "src": "/api/health", "dest": "/api/health.js" },
    { "src": "/api/(.*)", "dest": "/api/index.js" },
    { "src": "/$", "dest": "/src/index.html" },
    { "src": "/([^.]*)", "dest": "/src/index.html" },
    { "src": "/(.*)", "dest": "/src/$1" }
  ]
}
